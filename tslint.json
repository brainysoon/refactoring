{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
      "rules":{
        "no-console": [true, "log", "error"],
        "no-namespace": false,
        "object-literal-sort-keys": false,
        "ordered-imports": [false],
        "trailing-comma": [false],
        "quotemark":[false],
        "comment-format": [false],
        "max-classes-per-file":[false],
        "variable-name": [
            true, 
            "ban-keywords", 
            "check-format", 
            "allow-leading-underscore"
        ]
    },
    "rulesDirectory": []
}